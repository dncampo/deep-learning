import theano
import numpy as np
from logRegression import logRegression
rng = np.random

# TP2.1.a: 
a = theano.tensor.vector() 
out = a + a ** 10          
f = theano.function([a], out)
print(f([0, 1, 2]))

# a ** 2 + b ** 2 + 2 * a * b.
b = theano.tensor.vector() 
out = a**2 + b**2 + 2* a * b          
g = theano.function([a,b], out)
print(g([0, 1, 2],[0, 2, 3]))


